    "# Initailisation of firebase \n",
    "import firebase_admin\n",
    "from firebase_admin import credentials\n",
    "from firebase_admin import firestore\n",
    "from firebase_admin import db\n",
    "\n",
    "cred = credentials.Certificate(\"fb-key.json\") # auth-key\n",
    "firebase_admin.initialize_app(cred, {         # Initialisation\n",
    "    'databaseURL': 'https://let-s-upgrade-project.firebaseio.com' # project URL\n",
    "})\n",
    "db = firestore.client() "
   ]
