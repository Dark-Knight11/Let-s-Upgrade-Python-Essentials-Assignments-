    "import getpass # for hidden inputs\n",
    "import hashlib # for converting plain text password in hash format\n",
    "\n",
    "# Function for SigningUp new users \n",
    "def registerUser():\n",
    "    firebaseDB = db.collection(u'users') # created a db named users\n",
    "    # inputs for user\n",
    "    email = input(\"Enter your email - \") \n",
    "    firstName = input(\"Enter your First Name - \")\n",
    "    lastName = input(\"Enter your Last Name - \")\n",
    "    age = input(\"Enter your age - \")\n",
    "    passwd = getpass.getpass(prompt=\"Enter your password - \")\n",
    "    password = hashlib.md5(passwd.encode()).hexdigest() # password gets hashed\n",
    "    \n",
    "    # dictionary containing all user information\n",
    "    data = {}\n",
    "    data['email'] = email\n",
    "    data['FirstName'] = firstName\n",
    "    data['LastName'] = lastName\n",
    "    data['Age'] = age\n",
    "    data['Password'] = password\n",
    "    \n",
    "    firebaseDB.add(data) # uploading info on firebase\n",
    "    print(\"Your Account has been registered\")\n",
    "\n",
    "# Function to display information of all users \n",
    "def displayUsers():\n",
    "    dataBase = db.collection(u'users').stream() # reading data from firebase - users database\n",
    "    for data in dataBase:\n",
    "        # displaying all information\n",
    "        print(\"ID:\", data.id)\n",
    "        print(\"Email:\", data.to_dict().get(\"email\"))\n",
    "        print (\"First Name:\", data.to_dict().get(\"FirstName\"))\n",
    "        print (\"Last Name:\", data.to_dict().get(\"LastName\"))\n",
    "        print (\"Age:\", data.to_dict().get(\"Age\"))\n",
    "        print (\"Password:\", data.to_dict().get(\"Password\"))\n",
    "        print(\"--------------------------------------------------\")\n",
    "        print(\"--------------------------------------------------\")\n",
    "    print(\"\")\n",
    "    print(\"\")\n",
    "\n",
    "# Function for updating information\n",
    "def updateData():\n",
    "    # authenticating user before making changes\n",
    "    identity = input(\"Enter your email id - \")\n",
    "    passwd = getpass.getpass(prompt=\"Enter your password - \")\n",
    "    hashed = hashlib.md5(passwd.encode()).hexdigest()\n",
    "    docs = db.collection(u'users').stream()\n",
    "    for doc in docs:\n",
    "        # authentication code\n",
    "        if doc.to_dict().get(\"email\") == identity and doc.to_dict().get(\"Password\") == hashed:\n",
    "            uid = doc.id # storing auto-generated id from firebase which is required to update data \n",
    "            # updating options\n",
    "            while True:\n",
    "                print(\"What do you want to update?\")\n",
    "                print(\"1. Age\")\n",
    "                print(\"2. Email\")\n",
    "                print(\"3. Password\")\n",
    "                print(\"4. First Name\")\n",
    "                print(\"5. Last Name\")\n",
    "                print(\"6. Exit\")\n",
    "                option = input(\"Enter your choice - \")\n",
    "                updatedData = db.collection(u'users').document(uid) \n",
    "                if option == '1': # updates Age\n",
    "                    print(\"Current value:\", doc.to_dict().get(\"Age\"))\n",
    "                    updatedAge = input(\"Enter new Age - \")\n",
    "                    updatedData.update({\"Age\":updatedAge})\n",
    "                    print(\"\")\n",
    "                elif option == '2': # updates email\n",
    "                    print(\"Current value:\", doc.to_dict().get(\"email\"))\n",
    "                    updatedEmail = input(\"Enter new Email - \")\n",
    "                    updatedData.update({\"email\":updatedEmail})\n",
    "                    print(\"\")\n",
    "                elif option == '3': # updates password\n",
    "                    Pass = getpass.getpass(prompt=\"Enter new Password - \")\n",
    "                    updatedPass = hashlib.md5(Pass.encode()).hexdigest()\n",
    "                    updatedData.update({\"Password\":updatedPass})\n",
    "                    print(\"\")\n",
    "                elif option == '4': # updates First Name\n",
    "                    print(\"Current value:\", doc.to_dict().get(\"FirstName\"))\n",
    "                    updatedFN = input(\"Enter your new First Name - \")\n",
    "                    updatedData.update({\"FirstName\":updatedFN})\n",
    "                    print(\"\")\n",
    "                elif option == '5': # updates Last Name\n",
    "                    print(\"Current value:\", doc.to_dict().get(\"LastName\"))\n",
    "                    updatedLN = input(\"Enter your new Last Name - \")\n",
    "                    updatedData.update({\"LastName\":updatedLN})\n",
    "                    print(\"\")\n",
    "                elif option == '6': # exits the loop\n",
    "                    break\n",
    "                else:\n",
    "                    print(\"Enter valid option!!!\")\n",
    "                    print(\"\")\n",
    "            print(\"Your Data has been updated\")\n",
    "    print(\"\")\n",
    "    print(\"\")\n",
    "            \n",
    "# function for deleting data from database\n",
    "def deleteAccount():\n",
    "    ans = input(\"Do you want to delete your Account Permanently? Y or N - \")\n",
    "    if ans == 'Y':\n",
    "        # authentication before deleting\n",
    "        identity = input(\"Enter your email id - \")\n",
    "        passwd = getpass.getpass(prompt=\"Enter your password - \")\n",
    "        hashed = hashlib.md5(passwd.encode()).hexdigest()\n",
    "        docs = db.collection(u'users').stream()\n",
    "        for doc in docs:\n",
    "            if doc.to_dict().get(\"email\") == identity and doc.to_dict().get(\"Password\") == hashed:\n",
    "                uid = doc.id\n",
    "                answer = input(\"Are you sure? Y or N - \") \n",
    "                if answer == 'Y':\n",
    "                    db.collection(u'users').document(uid).delete()\n",
    "                    print(\"Your Account has been deleted\")\n",
    "    print(\"\")\n",
    "    print(\"\")\n",
    "\n",
    "# main code\n",
    "def myFirebase():\n",
    "    print(\"Welcome!!!\")\n",
    "    print(\"What do you want to do?\")\n",
    "    print(\"1. Register new users\")\n",
    "    print(\"2. View Data\")\n",
    "    print(\"3. Update Data\")\n",
    "    print(\"4. Delete Account\")\n",
    "    ans = input(\"Enter your choice - \")\n",
    "    print(\"\")\n",
    "    print(\"\")\n",
    "    if ans == '1':\n",
    "        registerUser()\n",
    "    elif ans == '2':\n",
    "        displayUsers()\n",
    "    elif ans == '3':\n",
    "        updateData()\n",
    "    elif ans == '4':\n",
    "        deleteAccount()"
   ]
